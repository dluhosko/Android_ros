<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_vibrotac_test"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:orientation="vertical"
    android:background="@color/cardview_dark_background"
    tools:context="com.example.viktordluhos.my_senso_teleop_gui.Vibrotac_test">

    <TextView
        android:text="Test Vibrotactile feedback actuators"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:textStyle="normal|bold"
        android:textAlignment="center"
        android:textColor="@android:color/white"
        android:textSize="25sp"
        android:layout_marginTop="22dp"
        android:id="@+id/textView13" />


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:weightSum="4"
        android:padding="5dp"
        android:orientation="horizontal">

        <Button
            android:layout_width="70dp"
            android:layout_height="75dp"
            android:id="@+id/button17"
            android:layout_weight="1"
            android:layout_marginEnd="10dp"/>

        <Button
            android:layout_width="150dp"
            android:layout_height="75dp"
            android:id="@+id/button16"
            android:onClick="onClick_upCenter"
            android:layout_weight="2"
            android:layout_marginEnd="10dp"/>

        <Button
            android:layout_width="70dp"
            android:layout_height="75dp"
            android:id="@+id/button18"
            android:onClick="onClick_upRight"
            android:layout_weight="1"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:weightSum="4"
        android:orientation="horizontal"
        android:padding="5dp">


        <Button
            android:layout_width="75dp"
            android:layout_height="150dp"
            android:id="@+id/button19"
            android:onClick="onClick_left"
            android:layout_weight="1"
            android:layout_marginEnd="10dp"/>

        <Button
            android:text="Test All"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:id="@+id/button_testAll"
            android:onClick="onClick_testAll"
            android:layout_weight="2"
            android:layout_marginEnd="10dp"/>

        <Button
            android:layout_width="75dp"
            android:layout_height="150dp"
            android:id="@+id/button20"
            android:onClick="onClick_right"
            android:layout_weight="1"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:weightSum="4"
        android:padding="5dp">

        <Button
            android:layout_width="75dp"
            android:layout_height="75dp"
            android:id="@+id/button22"
            android:onClick="onClick_downLeft"
            android:layout_weight="1"
            android:layout_marginEnd="10dp"/>

        <Button
            android:layout_width="150dp"
            android:layout_height="75dp"
            android:id="@+id/button21"
            android:onClick="onClick_downCenter"
            android:layout_weight="2"
            android:layout_marginEnd="10dp"/>

        <Button
            android:layout_width="75dp"
            android:layout_height="75dp"
            android:id="@+id/button23"
            android:onClick="onClick_downRight"
            android:layout_weight="1" />

    </LinearLayout>











</LinearLayout>
